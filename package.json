{
  "name": "json-ld.org",
  "private": true,
  "description": "json-ld.org homepage",
  "type": "module",
  "scripts": {
    "build": "eleventy",
    "build:editor": "rollup -c && cp playground/next/editor.bundle.js _site/playground/next/editor.bundle.js",
    "pages": "wrangler pages dev _site/ --compatibility-date=2025-04-02",
